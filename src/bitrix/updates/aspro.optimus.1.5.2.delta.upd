	    function json_encode($value){
	        if(is_int($value)){
				return (string)$value;
			}
			elseif(is_string($value)){
		        $value = str_replace(array('\\', '/', '"', "\r", "\n", "\b", "\f", "\t"),  array('\\\\', '\/', '\"', '\r', '\n', '\b', '\f', '\t'), $value);
		        $convmap = array(0x80, 0xFFFF, 0, 0xFFFF);
