	/*bind sale events*/
	foreach(GetModuleEvents("sale", "OnSaleComponentOrderOneStepComplete", true) as $arEvent)
		ExecuteModuleEventEx($arEvent, Array($orderID, $arOrder, $arParams));

	ob_clean();

	die(getJson($orderID, 'Y'));
}
