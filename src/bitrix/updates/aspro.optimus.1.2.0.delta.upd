					$CACHE_MANAGER->StartTagCache($cachePath);
					$CACHE_MANAGER->RegisterTag($cacheTag);
					$CACHE_MANAGER->EndTagCache();
				}

				$obCache->EndDataCache(array("arRes" => $arRes));
			}
		}

