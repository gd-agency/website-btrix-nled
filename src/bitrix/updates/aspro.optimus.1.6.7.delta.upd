			{
				const intCount = Math.round(
					Math.round(val * arOptimusOptions.JS_ITEM_CLICK.precisionFactor / ratio) / arOptimusOptions.JS_ITEM_CLICK.precisionFactor
				) || 1;
				val = intCount <= 1 ? ratio : intCount * ratio;
				val = Math.round(val*arOptimusOptions.JS_ITEM_CLICK.precisionFactor)/arOptimusOptions.JS_ITEM_CLICK.precisionFactor;
