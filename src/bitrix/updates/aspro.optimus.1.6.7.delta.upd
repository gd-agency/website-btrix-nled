			$(this.obCompare).find('.compare_item.to').hide();
			$(this.obCompare).find('.compare_item.added').show();
		}else{
			$(this.obCompare).find('.compare_item.added').hide();
			$(this.obCompare).find('.compare_item.to').show();
		}
	}
}

