				if(method_exists('CHTMLPagesCache', 'isOn')){
					if (CHTMLPagesCache::isOn()){
						if(isset($arHTMLCacheOptions['AUTO_COMPOSITE']) && $arHTMLCacheOptions['AUTO_COMPOSITE'] === 'Y'){
							return 'AUTO_COMPOSITE';
						}
						else{
							return 'COMPOSITE';
