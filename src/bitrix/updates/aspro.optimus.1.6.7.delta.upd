				}

				if( basketCount > 0 ){
					$('.basket-link.basket .count').removeClass('hidden')
					$('.basket-link.basket').removeClass('no-items')
				}else{
					$('.basket-link.basket .count').addClass('hidden')
					$('.basket-link.basket').addClass('no-items')
				}

