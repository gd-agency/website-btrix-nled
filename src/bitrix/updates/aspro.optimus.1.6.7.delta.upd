	foreach($arSites as $siteId => $arSite){
		$arSite['DIR'] = str_replace('//', '/', '/'.$arSite['DIR']);

		if(!strlen($arSite['DOC_ROOT'])){
			$arSite['DOC_ROOT'] = $_SERVER['DOCUMENT_ROOT'];
		}

		$arSite['DOC_ROOT'] = str_replace('//', '/', $arSite['DOC_ROOT'].'/');
		$siteDir = str_replace('//', '/', $arSite['DOC_ROOT'].$arSite['DIR']);
		$fileCheckSolution = str_replace('//', '/', $siteDir.'/ajax/form.php');

		if(file_exists($fileCheckSolution)){
			$content = @file_get_contents($fileCheckSolution);
			if(strpos($content, 'COptimus') !== false){
				$arPublicFiles = [
					'add' => [
						"/include/contacts-map-yandex.php",
						"/include/contacts-map-google.php",
					],
					'update' => [
						"/contacts/index.php",
						"/catalog/index.php",
					],
				];

				foreach($arPublicFiles as $type => $files) {
					foreach($files as $path){
						$file_from = $type === 'add'
							? str_replace('//', '/', $_SERVER['DOCUMENT_ROOT'].'/bitrix/modules/'.MODULE_NAME.'/install/wizards/'.PARTNER_NAME.'/'.MODULE_NAME_SHORT.'/site/public/ru'.$path)
							: str_replace('//', '/', $siteDir.$path);
						
						if(file_exists($file_from)) {
							$content = @file_get_contents($file_from);
							$file_to = str_replace('//', '/', $siteDir.$path);

							if( $type === 'update' ){
								switch($path){
									case '/contacts/index.php':
										$needle = "/\<\?[$]APPLICATION-\>IncludeComponent\(\s+\"bitrix:map\.(yandex|google)\.view\".*?\"MAP_DATA\"\s=>\s(\".*?\"),.*?\?\>/s";
										$replacement = "<?\$arMapTypes = ['YANDEX', 'GOOGLE'];\n\t\$sMapType = \$arMapTypes[\Bitrix\Main\Config\Option::get(\"aspro.optimus\", \"MAP_TYPE\", \"0\", SITE_ID)];\n?>\n\t<?\$APPLICATION->IncludeComponent(\"bitrix:main.include\", \".default\", \n\t\tarray(\n\t\t\t\"COMPONENT_TEMPLATE\" => \".default\",\n\t\t\t\"PATH\" => SITE_DIR.\"include/contacts-map-\".strtolower(\$sMapType).\".php\",\n\t\t\t\"AREA_FILE_SHOW\" => \"file\",\n\t\t\t\"AREA_FILE_SUFFIX\" => \"\",\n\t\t\t\"AREA_FILE_RECURSIVE\" => \"Y\",\n\t\t\t\"EDIT_TEMPLATE\" => \"standard.php\"\n\t\t),\n\t\tfalse\n\t);?>";
	
										preg_match($needle, $content, $matches);
	
										if( $matches[0] ){
											$content = preg_replace($needle, $replacement, $content);
											
											if( count($matches) === 3 ){
												$yMapIncPath = $_SERVER['DOCUMENT_ROOT']."/include/contacts-map-yandex.php";
												$gMapIncPath = $_SERVER['DOCUMENT_ROOT']."/include/contacts-map-google.php";
	
												$yMapContent = str_replace('google', 'yandex', $matches[0]);
												$gMapContent = str_replace('yandex', 'google', $matches[0]);
	
												@file_put_contents($yMapIncPath, $yMapContent);
												@file_put_contents($gMapIncPath, $gMapContent);
												$arMapTypes = [
													'yandex' => '0',
													'google' => '1'
												];

												Option::set(MODULE_NAME, 'MAP_TYPE', $arMapTypes[$matches[1]], $siteId);
											}
										}
										unset($matches);
									break;
									case '/catalog/index.php':
										$regex = [
											[
												'needle' => "/([$]APPLICATION->IncludeComponent\(\s+[\"|']bitrix:catalog[\"|'],\s+[\"|']main[\"|'],\s+array\()(.*?([\"|']SECTIONS_TYPE_VIEW[\"|']).*?;\?>)/s",
												'replacement' => "/([$]APPLICATION->IncludeComponent\(\s+[\"|']bitrix:catalog[\"|'],\s+[\"|']main[\"|'],\s+array\()(.*?;\?>)/s",
												'replacer' => "\n\t\t\"SECTIONS_TYPE_VIEW\" => \"FROM_MODULE\",",
											],
											[
												'needle' => "/([$]APPLICATION->IncludeComponent\(\s+[\"|']bitrix:catalog[\"|'],\s+[\"|']main[\"|'],\s+array\()(.*?([\"|']SECTION_TYPE_VIEW[\"|']).*?;\?>)/s",
												'replacement' => "/([$]APPLICATION->IncludeComponent\(\s+[\"|']bitrix:catalog[\"|'],\s+[\"|']main[\"|'],\s+array\()(.*?;\?>)/s",
												'replacer' => "\n\t\t\"SECTION_TYPE_VIEW\" => \"FROM_MODULE\",",
											],
										];
										foreach($regex as $value){
											preg_match($value['needle'], $content, $matches);
											
											if( !$matches ){
												preg_match($value['replacement'], $content, $matches);

												if( $matches ){
													$content = preg_replace_callback($value['replacement'], function($matches) use ($value){
														return $matches[1].$value['replacer'].$matches[2];
													}, $content);
												}
											}
											unset($matches);
										}
									break;
								}
							}

							CreateBakFile($file_to);
							@file_put_contents($file_to, $content);
							UpdaterLog('Update '.$path);
						}
