}

.loading-state {position: relative;}
.loading-state:before {
    content: "";
    display: block;
    background: rgba(255,255,255,0.5) url(../images/svg/double_ring.svg) center 50px no-repeat;
    position: absolute;
    z-index: 105;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
}


