$(document).on('mouseenter', '.menu_top_block>li:not(.full)', function(){

	if(window.matchMedia('(min-width: 951px)').matches && $(this).hasClass('catalog') && ( $('.banner_auto').hasClass('catalog_page') || $('.banner_auto').hasClass('front_page'))){
		return;
