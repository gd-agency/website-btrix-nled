				if(dropdownTop < dropdownTopMax) {
					dropdownTop = dropdownTopMax;
				}
				dropdown.css({
					'top': dropdownTop+'px',
					'bottom': -dropdownBottom+'px',
					'overflow': 'auto',
				});
			}
