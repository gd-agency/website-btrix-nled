	if ('RESET_FORM' in result){
		document.getElementById("smartfilter").reset();
		reset="Y";
	}

	if (!!result && !!result.ITEMS)
	{
		for(var PID in result.ITEMS)
		{
			if (result.ITEMS.hasOwnProperty(PID))
			{
				this.updateItem(PID, result.ITEMS[PID], reset);
			}
