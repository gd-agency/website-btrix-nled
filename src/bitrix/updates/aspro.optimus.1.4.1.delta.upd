    		$siteID = $_REQUEST['src_site'];
    	if(!$TEMPLATE_OPTIONS)
    		$TEMPLATE_OPTIONS = self::GetTemplateOptions($siteID);

    	$section_param = ((isset($arExtParams['SECTION']) && $arExtParams['SECTION']) ? $arExtParams['SECTION'] : 'SECTION');
