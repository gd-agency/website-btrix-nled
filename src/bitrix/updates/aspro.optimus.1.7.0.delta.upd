	{
		case 'GALLERY':
			break;
		case 'MAGNIFIER':
			$(this.obPict).addClass('zoom_picture');
			InitZoomPict();
			break;
		case 'POPUP':
			$(this.obPict).parent().addClass('fancy_offer');
