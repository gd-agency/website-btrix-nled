			$val = strlen($val) ? $val : $arOption[2];
		}

		if($name == 'CUSTOM_FONT')
		{
			$val = str_replace('<', '', $val);
			$val = str_replace('>', '', $val);

			$path = \Bitrix\Main\Application::getDocumentRoot().'/bitrix/components/aspro/theme.optimus/css/user_font_'.$siteID.'.css';
			$content = '';
