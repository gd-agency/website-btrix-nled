	if(arOptimusOptions["THEME"]["SHOW_TOTAL_SUMM"] == "ALWAYS")
	{
		var parent = 'body ';
		if(typeof popup  === 'string' && popup == 'Y')
			parent = '.popup ';
		$(parent+'.counter_wrapp .counter_block input.text').each(function(){
			var _th = $(this);

			if(_th.data('product'))
			{
