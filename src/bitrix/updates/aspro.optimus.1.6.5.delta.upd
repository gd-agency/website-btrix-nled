					};

					var listWrapper = $('.<?=$arPost["CLASS"]?>.js_offers__<?=$arPost["LINK_ID"]?> .bx_catalog_item_scu .item_wrapper:eq('+intNumber+') .list_values_wrapper');
					var max_scu_count = listWrapper.length ? listWrapper.data('maxCount') : 0;
