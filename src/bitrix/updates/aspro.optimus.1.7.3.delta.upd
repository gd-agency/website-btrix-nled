			if(
				isset($_FILES[$arOption[0]]) && (strlen($_FILES[$arOption[0]]['tmp_name']['n0']) || strlen($_FILES[$arOption[0]]['tmp_name']['0'])) ||
				(isset($_REQUEST[$arOption[0]]) && ($_REQUEST[$arOption[0]]['n0'] || $_REQUEST[$arOption[0]]))
				
			){
				$arValues = array();
